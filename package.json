{
  "name": "openapi-spec-parser",
  "version": "0.1.0",
  "description": "Transform OpenAPI specs into AI agent-optimized rules",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "openapi-parse": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "test:run": "vitest run",
    "test:generate": "rm -rf test/specs/output && node dist/cli/index.js generate -i test/specs/test-api.json -o test/specs/output",
    "lint": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "openapi",
    "swagger",
    "ai",
    "agent",
    "rules",
    "mcp",
    "rag"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "commander": "^12.0.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "openapi-types": "^12.1.3",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
